exports.datamain = data;
exports.datadepok = data_depok;

var array_kota = { "Depok": data_depok, "Bogor": data_bogor, "Jakarta": data_jakarta, "Semarang": data_semarang, "Surabaya": data_surabaya, "Yogyakarta": data_yogyakarta };

exports.getNavmenu = function () {
	var i;
	var dataNavMenu = [];
	for (i = 0; i < data.events.length; i++) {
		dataNavMenu.push([data.events[i].kota, data.events[i].kota]);
	}
	return dataNavMenu;
};


exports.getSubtitle = function (kota) {
	var dataKota = array_kota[kota];
	return dataKota;
};


exports.getAppIcon = function () {
	var i;
	var dataAppIcon = [];
	for (i = 0; i < data.gdg_icon_materi.length; i++) {
		dataAppIcon.push([data.gdg_icon_materi[i].icon, data.gdg_icon_materi[i].nama]);
	}
	return dataAppIcon;
};


exports.getFooterLogo = function () {
	var i;
	var logo = [];
	for (i = 0; i < data.events.length; i++) {
		logo.push(data.events[i].logo);
	}
	return logo;
};


exports.getPartnerLogo = function () {
	var i;
	var logo = [];
	for (i = 0; i < data.partners.length; i++) {
		logo.push(data.partners[i].logo);
	}
	return logo;
};


exports.getDaftarAcaraKota = function (kota, acara) {
	var i;
	var acaraKota = [];
	var dataKota = array_kota[kota];
	for (i = 0; i < dataKota[acara].length; i++) {
		acaraKota.push([dataKota[acara][i].nama, dataKota[acara][i].waktu, dataKota[acara][i].speaker]);
	}
	return acaraKota;
};

exports.getSpeakerKota = function (kota) {
	var i;
	var speakerKota = [];
	var dataKota = array_kota[kota];
	for (i = 0; i < dataKota['speakers'].length; i++) {
		speakerKota.push([dataKota.speakers[i].foto, dataKota.speakers[i].nama, dataKota.speakers[i].job]);
	}
	return speakerKota;
};

