exports.getRegisterAcara = function (kota) {

	var dataKota = array_kota[kota];
	var acaraRegister =
	[
	[dataKota.acara[0].nama, dataKota.acara[0].tempat, dataKota.acara[0].tgl, dataKota.acara[0].waktu, ["Register", dataKota.acara[0].link]],
	[dataKota.acara[1].nama, dataKota.acara[1].tempat, dataKota.acara[1].tgl, dataKota.acara[1].waktu, ["Register", dataKota.acara[1].link]]];

	return acaraRegister;
};